{
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "passThroughEnv": ["VITE_API_BASE_URL"],
      "env": ["VITE_API_BASE_URL"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "passThroughEnv": ["VITE_API_BASE_URL"],
      "env": ["VITE_API_BASE_URL"],
      "cache": true,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["VITE_API_BASE_URL"],
      "env": ["VITE_API_BASE_URL"]
    }
  }
}
